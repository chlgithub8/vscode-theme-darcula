{
    "name": "Sean Dark",
    "colors": {
        "editor.foreground": "#e1e4e8",
        "editor.background": "#242424",
        "peekViewEditor.background": "#1E1E1E",
        "peekViewResult.background": "#252526",
        "peekViewEditor.matchHighlightBackground": "#3A3A3A",
        "peekViewTitle.background": "#1E1E1E",
        "peekViewEditor.border": "#3A3A3A",
        
        "editorBracketMatch.background": "#3A3A3A",
        "editorBracketMatch.border": "#A9B7C6",
        
        "editorBracketHighlight.foreground1": "#A9B7C6",
        "editorBracketHighlight.foreground2": "#A9B7C6",
        "editorBracketHighlight.foreground3": "#A9B7C6",
        "editorBracketHighlight.foreground4": "#A9B7C6",
        "editorBracketHighlight.foreground5": "#A9B7C6",
        "editorBracketHighlight.foreground6": "#A9B7C6",
        
        "editorBracketPairGuide.activeBackground1": "#A9B7C6",
        "editorBracketPairGuide.activeBackground2": "#A9B7C6",
        "editorBracketPairGuide.activeBackground3": "#A9B7C6",
        "editorBracketPairGuide.activeBackground4": "#A9B7C6",
        "editorBracketPairGuide.activeBackground5": "#A9B7C6",
        "editorBracketPairGuide.activeBackground6": "#A9B7C6"
    },
    "semanticHighlighting": true,
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#629755"
            }
        },
        {
            "scope": [
                "keyword",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#CC7832"
            }
        },
        {
            "scope": [
                "entity.name.namespace",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#559BC4"
            }
        },
        {
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#E8AC51"
            }
        },
        {
            "scope": [
                "entity",
                "constant.other",
                "variable.other.constant",
                "variable.other.property",
                "variable.other.enummember",
                "entity.name.variable.enum-member",
                "entity.name.variable.enum"
            ],
            "settings": {
                "foreground": "#9876AA"
            }
        },
        {
            "scope": [
                "variable",
                "constant",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#A9B7C6"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#39B600"
            }
        },
        {
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#6A8759"
            }
        }
    ]
}